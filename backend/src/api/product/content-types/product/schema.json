{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "productName": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "productName"
    },
    "description": {
      "type": "string"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "active": {
      "type": "boolean"
    },
    "price": {
      "type": "decimal"
    },
    "discount": {
      "type": "decimal"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Womens",
        "Mens",
        "Unisex"
      ]
    },
    "isFeatured": {
      "type": "boolean"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    }
  }
}
